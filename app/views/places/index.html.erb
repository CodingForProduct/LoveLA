<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-3">
      <div class="well">
        <div class="stop-list-well">
          <% if @place.blank? %>
              <h3>No result found</h3>
          <% else %>
              <% @place.each do |row| %>
                  <%= link_to "#{row['name']}", places_show_path(row['id']), class: "list_data" %> <br />
              <% end %>
          <% end %>
        </div><!--div class="route-list-well"-->
      </div><!--div class="well"-->
    </div><!--div class="col-lg-8 col-lg-offset-3"-->
  </div><!--div class="row"-->
</div><!--div class="container"-->