<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-3">
      <div class="well">
        <div class="stop-list-well">
          <%#= @routelist.display_name %>
          <% @stoplist.each do |row| %>
            <%= link_to "#{row['display_name']}", places_path(:longitude =>"#{row['longitude']}", :latitude =>"#{row['latitude']}") %> <br />
          <% end %>
        </div><!--div class="route-list-well"-->
      </div><!--div class="well"-->
    </div><!--div class="col-lg-8 col-lg-offset-3"-->
  </div><!--div class="row"-->
</div><!--div class="container"-->
