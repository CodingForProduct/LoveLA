<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-3">
      <div class="well"><h1><%= @route['display_name'] %></h1>
        <div class="stop-list-well">
          <%#= @routelist.display_name %>
          <% @stoplist.each do |row| %>
            <%= link_to row['display_name'], stops_select_path(:stop => row['id'], :longitude => row['longitude'], :latitude =>row['latitude']), class: "list_data" %> <br />
          <% end %>
        </div><!--div class="route-list-well"-->
      </div><!--div class="well"-->
    </div><!--div class="col-lg-8 col-lg-offset-3"-->
  </div><!--div class="row"-->
</div><!--div class="container"-->
